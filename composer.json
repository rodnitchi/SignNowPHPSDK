{
    "name": "signnow/api-php-sdk",
    "license": "MIT",
    "description": "Library gives you ability to communicate with SignNow API",
    "type": "library",
    "keywords": ["php sdk", "api", "signnow"],
    "autoload": {
        "psr-4": {
            "SignNow\\Api\\": "src",
            "Examples\\": "examples/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rodnitchi/RestEntityManager"
        }
    ],
    "require": {
        "php": ">=7.1",
        "signnow/rest-entity-manager": "dev-guzzlefix"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
